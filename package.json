    "element-ui": "2.15.0",
    "file-saver": "2.0.1",
    "fuse.js": "3.4.4",
    "highcharts": "7.1.3",
    "highcharts-vue": "^1.3.5",
    "js-md5": "^0.7.3",
    "jsbarcode": "^3.11.0",
